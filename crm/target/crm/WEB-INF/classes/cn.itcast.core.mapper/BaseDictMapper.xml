<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.itcast.core.mapper.BaseDictMapper">
    <select id="findDictByTypeCode" parameterType="string" resultType="cn.itcast.core.pojo.BaseDict">
        SELECT * from base_dict WHERE dict_type_code=#{typeCode}
    </select>
</mapper>